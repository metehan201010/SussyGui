    game.Workspace.CurrentCamera.CFrame = (game.Players.LocalPlayer.Character.Head.CFrame * CFrame.Angles(math.rad(Camera),0,0)) * CFrame.new(0,SizeChange,0)
end)



 

     end,

 })

